// Hero section

.view-viewview.contenttype-event.has-hero-image{
  .ui.segment {
    padding-top: 0px !important;
    margin-top: 0px !important;
  }
}

//hide title
.view-viewview.hide-title {
  .header-bg {
    display: none !important;
  }

  .ui.basic.segment.content-area {
    margin-left: 0px !important;
  }

  .ui.segment {
    padding-top: 0px !important;
    margin-top: 0px !important;
  }

  #view {
    .ui.container {
      padding-top: 0px !important;
    }

    h1.documentFirstHeading {
      display: none !important;
    }

    .documentDescription {
      padding-bottom: 30px;
      text-align: left !important;
    }
  }
}

//hide top image
.view-viewview.hide-top-image:not(.contenttype-event) {
  .header-bg {
    display: none !important;
  }

  .logo-nav-wrapper.page-nav {
    display: block !important;
  }

  #view #page-document.ui.container {
    padding-top: 120px;
  }

  .documentFirstHeading {
    font-size: 60px !important;
    // padding-top: 43px !important;
    // margin-bottom: 10px !important;
    padding-top: 48px !important;
    margin-bottom: 17px !important;
    margin-top: 10px !important;
  }

  .documentDescription {
    max-width: 660px;
    padding-top: 0px;
    margin: 0 auto !important;
    color: #494a51;
    color: #494a51 !important;
    font-size: 1.5em;
    font-size: 24px !important;
    line-height: 36px !important;
    text-align: center !important;
    text-justify: inter-word !important;
  }
  @media (max-width: 992px) {
    .documentFirstHeading,
    .documentDescription {
      position: relative !important;
      right: 50% !important;
      left: 50% !important;
      width: 100vw !important;
      max-width: 100vw !important;
      padding-right: 130px !important;
      padding-left: 130px !important;
      margin-right: -50vw !important;
      margin-left: -50vw !important;
    }

    .documentFirstHeading {
      padding-top: 38px !important;
      margin-top: 15px !important;
      margin-bottom: 5px !important;
      margin-bottom: 18px !important;
      font-size: 45px !important;
      line-height: 42px !important;
    }

    .documentDescription {
      font-size: 18.8px !important;
      line-height: 28.32px !important;
    }
  }
  @media (max-width: 768px) {
    .documentFirstHeading {
      padding-top: 8px !important;
      margin-bottom: 18px !important;
      text-align: center !important;
      line-break: anywhere !important;
    }
    #view .documentDescription:not(:empty){
      margin-top: 10px !important;
      margin-bottom: 0px !important;
    }
  }
  @media (max-width: 480px){
    .documentFirstHeading, .documentDescription {
      padding-left: 55px !important;
      padding-right: 55px !important;
    }
  }
}

.view-viewview.hide-top-image.contenttype-event{
  #main{
    #page-document{
      @media (max-width: 768px){
        padding-left: 35px !important;
        padding-right: 35px !important;
      }
    }
    .documentDescription {
      @media (max-width: 768px) {
        padding-right: 30px !important;
        padding-left: 30px !important;
      }
      margin-top: 10px !important;
      padding-left: 0px !important;
    }
    .documentFirstHeading{
      @media (max-width: 768px){
        font-size: 40px !important;
        padding-left: 30px !important;
        padding-right: 30px !important;
      }
      @media (max-width: 992px){
        margin-top: 55px;
      }
      @media (min-width: 1200px) {
        margin-left: 101px !important;
      }
    }
    .hero-dates-wrapper{
      display: none;
      @media (min-width: 992px) {
        display: block !important;
        margin-top: 18px !important;
      }
      @media (min-width: 1200px){
        max-width: 100%;
        padding-left: 101px !important;
      }
      .hero-dates {
        color: #404040 !important;
      }
    }
  }
}


//Hero title
.description-container,
.description-container > p {
  z-index: 3;
  width: 100%;
  max-width: 650px;
  margin-top: 5px;
  margin-right: auto;
  margin-bottom: 5px;
  margin-left: auto;
  color: #ffffff !important;
  font-family: 'FranklinBook', Arial, sans-serif;
  font-size: 19px;
  font-weight: 500;
  letter-spacing: normal;
  line-height: 32px;
  text-align: center;

  .content-description {
    width: 100%;
    margin-right: auto;
    margin-left: auto;
    line-height: 1.6;
    text-align: center;

    @media (max-width: 991px) {
      max-width: 80%;
      margin-top: 5px;
    }
  }
  // @media (max-width: 768px) {
  //   display: none;
  // }
}

.herosection-content-wrapper {
  position: relative;

  .buttons{
    display: flex;
    padding-bottom: 10px;
    justify-content: center;
  }

  .ticket-button {
    width: auto;
    padding-top: 19px;
    padding-bottom: 15px;
    padding-right: 28px;
    padding-left: 28px;
    border: 1px solid #ffffff !important;
    // margin: 10px 20px !important;
    margin-right: 2.5px !important;
    margin-left: 2.5px !important;
    color: #ffffff !important;
    font-family: 'FranklinMed', Arial, sans-serif;
    font-size: 17px;
    letter-spacing: 0.05em;
    text-align: center;
    text-transform: uppercase;
    background-color: transparent !important;
  }

  .ticket-button:hover {
    border: 1px solid #282931 !important;
    background-color: #282931 !important;
    color: #fff;
    cursor: pointer;
  }

  @media (min-width: 992px) {
    .ticket-button {
      color: #000000;
    }

    .ticket-button:hover {
      border: 1px solid #282931 !important;
      background-color: #282931 !important;
    }
  }
}

.header-title-dates {
  position: absolute;
  z-index: 3;
  bottom: 25px;
  width: 100%;
  padding-right: 95px;
  padding-left: 95px;

  @media (max-width: 990px) {
    padding-right: 15vw;
    padding-left: 15vw;

    .description-container {
      // width: 563px;
      // max-width: 80% !important;
    }
  }

  @media (max-width: 768px) {
    padding-right: 35px;
    padding-left: 35px;
  }
}

.hero-title-floating {
  z-index: 3;
  width: 100%;
  max-width: 100%;
  margin-top: 15px !important;
  margin-bottom: 0px;
  color: #ffffff;
  font-family: 'FranklinDmCp', Arial, sans-serif;
  font-size: 76px;
  line-height: 70px !important;
  text-align: center !important;

  @media (max-width: 991px) {
    font-size: 86px;
    line-height: 70px !important;
  }

  @media (max-width: 768px) {
    font-size: 60px !important;
    letter-spacing: 0.05em !important;
    line-height: 58px !important;
  }
}

.hero-dates-wrapper {
  width: 100%;
  margin-right: auto;
  margin-left: auto;
  text-align: center;
}

.expired .hero-dates {
  text-decoration: line-through;
}

.hero-dates {
  margin-bottom: 10px;
  color: #fff;
  font-family: 'FranklinMed', Arial, sans-serif;
  font-size: 17px;
  letter-spacing: 0.05em;
  line-height: 35px;
  text-align: center;
  text-transform: uppercase;

  @media (max-width: 768px) {
    font-size: 14px;
    line-height: 20px;
  }
}

// hero title over
.herosection {
  height: 140px;
}

.has-hero-image .herosection,
.herosection img {
  height: 90vh;
  min-height: 400px;
  max-height: 720px;

  @media (min-width: 1640px) {
    max-height: 1000px !important;
  }
}

.herosection-content-image {
  position: relative;
  z-index: 0;
  z-index: 2;
  display: flex;
  width: 100%;
  height: 90vh;
  min-height: 400px;
  max-height: 720px;
  align-items: flex-end;
  margin: 0;

  @media (min-width: 1640px) {
    max-height: 1000px !important;
  }

  picture {
    display: flex;
    width: 100%;
    height: 100%;
    align-items: center;
    justify-content: center;
  }

  img {
    min-width: 100%;
    min-height: 100%;
    object-fit: cover;
    object-position: center;
  }

  figcaption {
    position: absolute;
    z-index: 3;
    top: 100%;
    right: 0;
    padding-top: 4px;
    padding-right: 10px;
    padding-bottom: 4px;
    padding-left: 10px;
    margin-right: 0;
    background: transparent;
    caption-side: bottom;
    font-size: 14px;
    line-height: 18px;
    line-height: 2;
    opacity: 1;
    text-align: right;
  }

  &::after {
    position: absolute;
    top: 0;
    left: 0;
    display: block;
    width: 100%;
    height: 100%;
    background-image: linear-gradient(
      to top,
      rgba(0, 0, 0, 0.7) 0%,
      rgba(0, 0, 0, 0) 50%
    );
    background-repeat: repeat-x;
    content: '';
    opacity: 0.9;
  }

  .ui.container {
    z-index: 1;
  }

  h1 {
    z-index: 1;
    max-width: 842px;
    padding-right: 40px;
    padding-bottom: 20px;
    padding-left: 40px;
    border-bottom: 0;
    margin: 0 0 10px;
    margin-bottom: 10px;
    color: #fff;
    font-size: 80px;
    letter-spacing: 0;
    line-height: 1;
    text-align: left;
  }
}

