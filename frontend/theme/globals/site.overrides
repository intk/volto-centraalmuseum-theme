body {
  display: block !important;
  color: #404040;
  font-family: 'FranklinBook', Arial, sans-serif;
  font-size: 16px;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  overflow-x: hidden;
  -webkit-tap-highlight-color: #0085a1;
}
@media screen and (min-width: 1200px) {
  .view-viewview.contenttype-document
    .ui.basic.segment.content-area
    #page-document
    > *:not(div, .documentFirstHeading),
  .view-viewview.contenttype-document
    .ui.basic.segment.content-area
    #page-document
    > .block.image.align,
  .view-viewview.contenttype-document
    .ui.basic.segment.content-area
    #page-document
    > .block.video.align,
  .view-viewview.contenttype-document
    .ui.basic.segment.content-area
    #page-document
    > .accordion-block,
  .view-viewview.contenttype-document
    .ui.basic.segment.content-area
    #page-document
    > .block.__button,
  .view-viewview.contenttype-event
    .ui.basic.segment.content-area
    #page-document
    > *:not(div, .documentFirstHeading),
  .view-viewview.contenttype-event
    .ui.basic.segment.content-area
    #page-document
    > .block.image.align,
  .view-viewview.contenttype-event
    .ui.basic.segment.content-area
    #page-document
    > .block.video.align,
  .view-viewview.contenttype-event
    .ui.basic.segment.content-area
    #page-document
    > .accordion-block,
  .view-viewview.contenttype-event
    .ui.basic.segment.content-area
    #page-document
    > .block.__button,
  .view-viewview.contenttype-exhibition
    .ui.basic.segment.content-area
    #page-document
    > *:not(div, .documentFirstHeading),
  .view-viewview.contenttype-exhibition
    .ui.basic.segment.content-area
    #page-document
    > .block.image.align,
  .view-viewview.contenttype-exhibition
    .ui.basic.segment.content-area
    #page-document
    > .block.video.align,
  .view-viewview.contenttype-exhibition
    .ui.basic.segment.content-area
    #page-document
    > .accordion-block,
  .view-viewview.contenttype-exhibition
    .ui.basic.segment.content-area
    #page-document
    > .block.__button,
  .view-viewview
    .ui.basic.segment.content-area
    #page-document
    > .event-recurrence-dates {
    box-sizing: border-box !important;
    margin-left: 101px;
  }

  .hide-top-image .documentDescription {
    padding-right: 50px;
    padding-left: 50px;
  }
}

html {
  scroll-behavior: smooth;
}

#page-document.ui.container {
  width: calc(@container-width - 100px) !important;
  @media screen and (min-width: 1200px) {
    width: @container-width !important;
  }
}
@media screen and (max-width: 768px) {
  #page-document.ui.container,
  #page-edit.ui.container {
    width: 100% !important;
    padding-right: 0% !important;
    padding-left: 0% !important;
    margin: 0% !important;
  }

  #page-document.ui.container > *:not(.block.image.align, .block.video.align),
  #page-edit.ui.container > *:not(.block.image.align, .block.video.align) {
    padding-right: 30px;
    padding-left: 30px;
  }

  #page-document.ui.container > ol,
  #page-document.ui.container > ul {
    padding-left: 70px !important;
  }
}

p {
  margin: 30px 0;
  line-height: 1.5;
}

#view,
#page-edit,
#page-search {
  ::-moz-selection {
    background: @brand-primary;
    color: white;
    text-shadow: none;
  }

  ::selection {
    background: @brand-primary;
    color: white;
    text-shadow: none;
  }

  img::selection {
    background: transparent;
    color: white;
  }

  img::-moz-selection {
    background: transparent;
    color: white;
  }

  .image-wrapper-,
  .image-wrapper-large,
  .image-wrapper-medium,
  .image-wrapper-small,
  .block.video.align {
    margin-top: 0px;
    margin-bottom: 70px;
  }

  a {
    border-bottom: 1px solid black;
    color: @brand-primary-text;
    text-decoration: none;
  }

  a:hover,
  a:focus {
    border-bottom: 1px solid @brand-primary-red !important;
    color: @brand-primary-red;
  }

  h1,
  h2,
  h3,
  h4,
  p {
    color: @brand-primary;
    font-family: 'FranklinBook', Arial, sans-serif;
  }

  h1.documentFirstHeading {
    max-width: 100%;
    border-bottom: 0px;
    margin-top: 15px;
    margin-right: auto;
    margin-bottom: 18px;
    margin-left: auto;
    color: #282931;
    font-family: 'FranklinDmCp', Arial, sans-serif;
    font-size: 80px;
    font-weight: 400;
    letter-spacing: 0em;
    line-height: 62px;
    text-align: center;
    text-transform: none;
  }

  .documentFirstHeading::before {
    display: none;
  }
  @media screen and (max-width: 678px) {
    h1.documentFirstHeading {
      font-size: 40px;
      line-height: 35px;
      text-align: left;
    }
  }

  // Description
  .documentDescription:not(:empty) {
    display: block;
    max-width: 100%;
    margin-top: 78.4px;
    margin-right: auto;
    margin-bottom: 18px;
    margin-left: auto;
    color: #282931;
    font-family: 'FranklinBook', Arial, sans-serif;
    font-size: 26.5px;
    letter-spacing: 0em;
    line-height: 42px;
    text-align: left;

    @media screen and (max-width: 768px) {
      margin-top: 104px !important;
      margin-bottom: 33px !important;
      font-size: 24px;
    }
  }
  //

  h2 {
    font-family: 'FranklinBook', Arial, sans-serif;
    font-size: 30px;
  }

  h3 {
    margin-top: 45px;
    margin-bottom: 10px;
    color: #494a51;
    font-family: 'FranklinMed', Arial, sans-serif;
    font-size: 25px;
    font-weight: 400;
    letter-spacing: 0em;
    line-height: 32px;
    text-transform: none;
  }
  @media screen and (max-width: 1200px) {
    h3 {
      margin-top: 20px;
    }
  }
  @media screen and (max-width: 768px) {
    h3 {
      font-size: 21.6px;
      line-height: 32px;
    }
  }

  p {
    margin: 8px 0;
    margin-bottom: 35px;
    color: #494a51;
    font-family: 'FranklinBook', Arial, sans-serif;
    font-size: 19.68px;
    letter-spacing: 0em;
    line-height: 36px;
  }

  @media screen and (max-width: 768px) {
    p {
      font-size: 18.88px;
    }
  }

  #page-document a {
    border-bottom: 1px solid #494a51;
  }

  #page-document .image-wrapper- a {
    border-bottom: 0px !important;
  }

  blockquote {
    padding: 1em;
    border-left: 1em solid #ccc;
    margin: 8px 0;
    margin-bottom: 35px;
    background: #eee !important;
    clear: both;
    color: #494a51 !important;
    font-family: 'FranklinBook', Arial, sans-serif;
    font-size: 19.68px;
    letter-spacing: 0em;
    line-height: 36px;
  }

  ol,
  ul {
    padding-left: 40px;
    color: #494a51;
    font-size: 18px;
    line-height: 37px;

    li {
      padding-left: 10px;
    }
  }
}

// Logo
.logo-nav-wrapper {
  position: relative;
  top: 50px;
  left: -180px;
  display: none;

  a {
    pointer-events: auto;
  }
}

// ************************
// About the size of the Images and the Videos on wide screens
@media screen and (min-width: 1200px) {
  .view-viewview .block.align.center {
    text-align: center;

    iframe,
    img:not(.placeholder, .medium, .small),
    .video-inner {
      max-width: 130%;
      margin-right: auto;
      margin-left: 50%;
      transform: translateX(-61.5%);

      // &.medium {
      //   max-width: 50%;
      // }

      // &.small {
      //   max-width: 25%;
      // }
    }

    iframe {
      width: 130%;
    }

    .video-inner {
      width: 930px;
      margin-left: 50%;
      transform: translateX(-61.5%);
    }
  }

  .block.image.align.center {
    padding-top: 45px !important;
  }

  .view-viewview .columns-view .block.image.align.center {
    img {
      max-width: 100% !important;
    }
  }
}

#view, #page-edit{
  p.callout {
    font-size: 15px;
    color: #777777;
    margin-top: 0px;
    border: none;
    box-shadow: none;
    padding-left: 0px;
    @media screen and (max-width: 1200px) {
      font-size: 12px;
    }
  }
}

// image link section
#view #page-document image-wrapper a {
  border-bottom: none;
}

//image credits section
.view-viewview {
  position: relative;

  .block.video.align {
    #photo-credit {
      position: absolute;
      left: -30%;
    }
  }

  .block.image.align.center {
    .image-wrapper-large,
    .image-wrapper- {
      #photo-credit {
        position: absolute;
        left: -30%;
      }
    }

    .image-wrapper-medium {
      #photo-credit {
        position: absolute;
        left: 25%;
      }
    }

    .image-wrapper-small {
      #photo-credit {
        position: absolute;
        left: 38%;
      }
    }
  }
  @media screen and (max-width: 1200px) {
    //image credits section
    .block.video.align {
      #photo-credit {
        position: absolute;
        left: 0%;
      }
    }

    .block.image.align.center {
      .image-wrapper-large,
      .image-wrapper- {
        #photo-credit {
          position: absolute;
          left: 0%;
        }
      }
    }
  }
  @media screen and (max-width: 768px) {
    //image credits section
    .block.video.align {
      #photo-credit {
        position: absolute;
        left: 5%;
      }
    }

    .block.video.align,
    .block.image.align.center {
      .image-wrapper-large,
      .image-wrapper- {
        #photo-credit {
          position: absolute;
          left: 5%;
        }
      }
    }
  }
}

#view {
  .block.image.align.right {
    .image-wrapper- {
      max-width: 50%;
      padding-top: 1em !important;
      margin-right: 1em !important;
      margin-bottom: 0px !important;
      margin-left: 1em !important;
      float: right;

      img {
        max-width: 100%;
        padding-top: 0px !important;
      }

      #photo-credit {
        margin-bottom: 0px !important;
      }
    }
  }

  .block.image.align.left {
    .image-wrapper- {
      max-width: 50%;
      padding-top: 0px !important;
      padding-top: 1em !important;
      margin-right: 1em !important;
      margin-bottom: 0px !important;
      margin-left: 1em !important;
    }

    img {
      max-width: 100%;
      padding-top: 0px !important;
    }

    #photo-credit {
      margin-bottom: 0px !important;
    }
  }
}

.block.image.align.left,
.block.image.align.right {
  img {
    &.large {
      max-width: 100% !important;
    }

    &.medium {
      max-width: 50% !important;
    }

    &.small {
      max-width: 25% !important;
    }
  }
}

//image credits alignment
div#photo-credit {
  display: block;
  margin-top: 5px !important;
  margin-bottom: 10px;
  color: rgb(102, 102, 102);
  color: #777777 !important;
  font-size: 15px !important;
  line-height: 23px !important;
  text-align: left;
  @media (max-width: 1200px) {
    margin-top: 0px !important;
    font-size: 12px !important;
  }
}

// Table block
.slate-table-block {
  thead p {
    letter-spacing: 0.05em !important;
  }

  border: 0px !important;

  th,
  td {
    padding-top: 10px !important;
    padding-bottom: 5px !important;
    line-height: 32px !important;
  }
  // td:last-child{
  //   text-align: right !important;
  // }
}
.slate-table-block{
    @media screen and (min-width: 1200px) {
        width: 650px !important;
      }
}
.slate-table-block * {
  border-right: 0px !important;
  border-left: 0px !important;
  margin: 0% !important;
  background-color: #fff !important;
  font-size: 17px !important;

  td > div {
    color: #494a51;
    font-family: 'FranklinBook', Arial, sans-serif;
    letter-spacing: 1.275px !important;
    line-height: 32px !important;
  }

  p {
    letter-spacing: 1.275px !important;
  }
}

.slate-table-block td {
  border-top: 1px black solid;
  border-bottom: 0.625px rgb(234, 234, 234) solid;
}

.slate-table-block{
  tr{
    td{
      padding-left: 0px !important;
      padding-right: 20px !important;
    }
    td > p {
      line-height: 32px !important;
    }
    td:nth-child(1){
      width: 161.180px;
    }
  }
  tr:first-child{
    td {
      padding-bottom: 0px !important;
    }
    td, td > p, td > div{
      font-family: 'FranklinBook', Arial, sans-serif;
      // text-transform: uppercase;
      font-size: 17px !important;
      line-height: 22px !important;
      letter-spacing: 0.85px !important;
      color: #666666 !important;
      vertical-align: middle;
      padding-left: 0px;
      padding-right: 20px;
      padding-top: 15px;
      padding-bottom: 10px;
    }
  }
}

// ******************
.ui.styled.accordion .title {
  background-color: #fbfbfb !important;
}

// Button block
.button.container .ui.button {
  display: block;
  display: inline-block;
  width: auto;
  height: auto;
  padding: 17px;
  padding-right: 28px;
  padding-bottom: 12px;
  padding-left: 28px;
  border: 1px solid #282931;
  margin: 0 auto;
  margin-left: auto;
  background-color: #ffffff;
  border-radius: 0px;
  color: #282931;
  font-family: 'FranklinMed', Arial, sans-serif;
  font-size: 17px;
  font-weight: 400;
  letter-spacing: 0em;
  letter-spacing: 0.05em;
  line-height: 27px;
  text-shadow: 0px 0px 0px #ffffff;
  text-transform: none;
}

.button.container .ui.button:hover {
  background-color: black !important;
  color: white !important;
  cursor: pointer !important;
}
